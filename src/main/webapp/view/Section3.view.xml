<mvc:View controllerName="sap.ui.demo.controller.Detail" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:u="sap.ui.unified">
	<Panel headerText="Code Quality">
		<FlexBox class="testResultBox2" alignItems="Start" alignContent="SpaceAround">
			<items>
				<Panel headerText="{i18n>section3Module1}" width="24em">
					<Panel class="testPanel" expandable="true" headerText="Number of Issues: {path:'total>/total'} Average:{path:'total>/average'}" width="auto">
						<Text text="{path:'model1>message'}"/>
						<Table class="sapUiSmallMarginEnd" items="{ path : 'asset>/', sorter: { path: 'Type' } }">
							<columns>
								<Column minScreenWidth="Tablet">
									<Text text="Product"/>
								</Column>
								<Column minScreenWidth="Tablet" demandPopin="true">
									<Text text="Supplier"/>
								</Column>
								<Column minScreenWidth="Tablet" demandPopin="true">
									<Text text="Dimensions"/>
								</Column>
							</columns>
							<items>
								<ColumnListItem>
									<cells>
										<Text text="{asset>ProductID}"/>
										<Text text="{asset>ProductName}"/>
										<Text text="TEXT"/>
									</cells>
								</ColumnListItem>
							</items>
						</Table>
					</Panel>
				</Panel>
				<Panel headerText="{i18n>section3Module2}" width="24em">
					<Panel class="testPanel" expandable="true" headerText="Number of Issues: {path:'total>/total'} Average:{path:'total>/average'}" width="auto">
						<Text text="{path:'model1>message'}"/>
						<Table class="sapUiSmallMarginEnd" items="{ path : 'asset>/'}">
							<columns>
								<Column minScreenWidth="Tablet">
									<Text text="Product"/>
								</Column>
								<Column minScreenWidth="Tablet" demandPopin="true">
									<Text text="Supplier"/>
								</Column>
								<Column minScreenWidth="Tablet" demandPopin="true">
									<Text text="Dimensions"/>
								</Column>
							</columns>
							<items>
								<ColumnListItem>
									<cells>
										<Text text="{asset>ProductID}"/>
										<Text text="{asset>ProductName}"/>
										<Text text="TEXT"/>
									</cells>
								</ColumnListItem>
							</items>
						</Table>
					</Panel>
				</Panel>
				<Panel headerText="{i18n>section3Module3}" width="42em">
					<Panel class="testPanel" expandable="true" headerText="Number of Issues: {path:'total>/total'} Average:{path:'total>/average'}" width="auto">
						<Text text="{path:'model1>message'}"/>
						<Table class="sapUiSmallMarginEnd" items="{ path : 'asset>/'}">
							<columns>
								<Column minScreenWidth="Tablet">
									<Text text="Product"/>
								</Column>
								<Column minScreenWidth="Tablet" demandPopin="true">
									<Text text="Supplier"/>
								</Column>
								<Column minScreenWidth="Tablet" demandPopin="true">
									<Text text="Dimensions"/>
								</Column>
							</columns>
							<items>
								<ColumnListItem>
									<cells>
										<Text text="{asset>ProductID}"/>
										<Text text="{asset>ProductName}"/>
										<Text text="TEXT"/>
									</cells>
								</ColumnListItem>
							</items>
						</Table>
					</Panel>
				</Panel>
			
			</items>
		</FlexBox>
	</Panel>
</mvc:View>